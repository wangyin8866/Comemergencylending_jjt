<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="maintab_toolbar_button">
        <item name="android:layout_width">0dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">11sp</item>
        <item name="android:drawablePadding">2dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@drawable/maintab_text_color_selector</item>
    </style>

    <!-- 页面底部的点击按钮样式 bu huwei 2013.12.26 -->
    <style name="tv_button">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:background">@drawable/checkbox_selector</item>

        <item name="android:button">@color/transparent</item>
    </style>

    <style name="amount_CheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:background">@drawable/amount_credit_selector</item>
        <item name="android:button">@color/transparent</item>
    </style>

    <!--我的界面-->
    <style name="message_authentication_lineraout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="horizontal_line">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/backgroud</item>
    </style>

    <style name="vertical_line">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@color/line</item>
    </style>

    <!--借款用途界面-->
    <style name="usage_of_loan_relativelayout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="usage_of_loan_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="home_popupmenu_animation">
        <item name="android:windowEnterAnimation">@anim/home_pop_menu_enter</item>
        <item name="android:windowExitAnimation">@anim/home_pop_menu_exit</item>
    </style>

    <style name="transparentFrameWindowStyle" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/photo_choose_bg</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="main_menu_animstyle">
        <item name="android:windowEnterAnimation">@anim/photo_dialog_in_anim</item>
        <item name="android:windowExitAnimation">@anim/photo_dialog_out_anim</item>
    </style>

    <style name="dialog_usage">
        <item name="android:windowEnterAnimation">@anim/transparent_in</item>
        <item name="android:windowExitAnimation">@anim/transparent_out</item>
    </style>

    <style name="spacestyle">
        <item name="android:gravity">left</item>
        <item name="android:textColor">@color/name</item>
        <item name="android:minEms">6</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="spacecenter">
        <item name="android:gravity">center_vertical</item>
        <item name="android:minEms">6</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="spacepadding14">
        <item name="android:gravity">center_vertical</item>
        <item name="android:minEms">6</item>
        <item name="android:paddingLeft">15dp</item>

    </style>

    <style name="spacepadding16">
        <item name="android:gravity">center_vertical</item>
        <item name="android:minEms">6</item>
        <item name="android:paddingLeft">16dp</item>
    </style>

    <style name="spacepadding18">
        <item name="android:gravity">center_vertical</item>
        <item name="android:minEms">6</item>
        <item name="android:paddingLeft">16dp</item>
    </style>
    <!--分享邀请图片-->
    <style name="share_invite_img">
        <item name="android:layout_width">50dip</item>
        <item name="android:layout_height">50dip</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <!--分享邀请文字-->
    <style name="share_invite">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">20dip</item>
        <item name="android:layout_marginTop">5dip</item>
        <item name="android:textColor">@color/home_loan_period_textcolor</item>
        <item name="android:textSize">15sp</item>
        <item name="android:typeface">normal</item>
        <item name="android:gravity">center</item>
        <item name="android:visibility">visible</item>
    </style>
    <!--借款确认外层-->
    <style name="confirmation_loan_outer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:background">@color/white</item>
    </style>
    <!--借款确认左-->
    <style name="confirmation_loan_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/amont_of_loan_textcolor</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>
    <!--借款确认右-->
    <style name="confirmation_loan_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/amont_of_loan_textcolor</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>


    <!--按钮-->
    <style name="button_guide">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button_guide_selector</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <!--按钮-->
    <style name="button_orange">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button_orange_selector</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <!--按钮-->
    <style name="button_grey">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button_grey_selector</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <!--按钮-->
    <style name="button_orange1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">52dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button_orange_selector</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <!--网页进度条-->
    <style name="Widget.MaterialProgressBar.ProgressBar.Horizontal" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:indeterminateDrawable">@null</item>
        <item name="android:minHeight">4dp</item>
        <item name="android:maxHeight">4dp</item>
    </style>
    <!--橙色字体-->
    <style name="text_orange">
        <item name="android:textColor">@color/orange</item>
    </style>
    <!--默认字体-->
    <style name="text_default">
        <item name="android:textColor">@color/light_grey</item>
    </style>
    <!--进度样式-->
    <style name="process">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/title</item>
        <item name="android:background">@color/transparent</item>
    </style>
    <!--密码隐藏-->
    <style name="password_hide">
        <item name="android:layout_width">28dp</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="SeekBar">
        <item name="android:paddingLeft">15.0dip</item>
        <item name="android:paddingRight">15.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5.0dip</item>
        <item name="android:layout_marginBottom">5.0dip</item>
        <item name="android:maxHeight">@dimen/seekbar_progress_height</item>
        <item name="android:progressDrawable">@drawable/progressbar_bg</item>
        <item name="android:minHeight">@dimen/seekbar_progress_height</item>
        <item name="android:thumb">@drawable/seekbar_thumb</item>
        <item name="android:isScrollContainer">true</item>
        <item name="android:fadeScrollbars">true</item>
    </style>
    <style name="AnimTools" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_in</item>
        <item name="android:windowExitAnimation">@anim/push_out</item>
    </style>
</resources>
