<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/present_store_company"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="15dp"
        android:text=""
        android:textColor="@color/name"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/present_store_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/present_store_company"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@+id/present_store_company"
        android:text=""
        android:textColor="@color/register_hint"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/present_store_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/present_store_company"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:text=""
        android:textColor="@color/home_loan_period_textcolor"
        android:textSize="15dp" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="@color/register_line" />
</RelativeLayout>
